{
  "name": "@genwave/svgmaker-mcp",
  "version": "0.1.1",
  "description": "MCP server for generating, editing, and converting SVG images using SVGMaker API",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "svgmaker-mcp": "build/index.js"
  },
  "directories": {
    "doc": "docs"
  },
  "files": [
    "build",
    "docs"
  ],
  "scripts": {
    "build": "rimraf build && tsc",
    "start": "node build/index.js",
    "dev": "ts-node-dev --esm src/index.ts",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "mcp",
    "svg",
    "svgmaker",
    "ai",
    "image-generation",
    "svg-editor",
    "svg-converter",
    "vector-graphics",
    "image-processing"
  ],
  "author": "Genwave",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "dotenv": "^16.5.0",
    "@genwave/svgmaker-sdk": "^0.2.2",
    "zod": "^3.25.46"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "rimraf": "^6.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
